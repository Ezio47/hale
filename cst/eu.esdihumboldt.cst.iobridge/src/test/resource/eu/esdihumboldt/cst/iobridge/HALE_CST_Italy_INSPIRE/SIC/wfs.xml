          <xsd:element maxOccurs="1" minOccurs="0" name="REGIONE" nillable="true" type="xsd:string"/>
          <xsd:element maxOccurs="1" minOccurs="0" name="SUBSITO" nillable="true" type="xsd:string"/>
          <xsd:element maxOccurs="1" minOccurs="0" name="NOME_SITO" nillable="true" type="xsd:string"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="SIC" substitutionGroup="gml:_Feature" type="Italy:SICType"/>
</xsd:schema>