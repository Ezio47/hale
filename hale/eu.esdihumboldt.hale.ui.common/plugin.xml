<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="eu.esdihumboldt.hale.ui.common.functions.ListFunctionsHandler"
            commandId="eu.esdihumboldt.hale.ui.common.functions.list">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="eu.esdihumboldt.hale.ui.common.functions.list"
            name="List available functions">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.services">
      <serviceFactory
            factoryClass="eu.esdihumboldt.hale.ui.common.definition.internal.ServiceFactory">
         <service
               serviceClass="eu.esdihumboldt.hale.ui.common.definition.AttributeEditorFactory">
         </service>
         <service
               serviceClass="eu.esdihumboldt.hale.ui.common.definition.DefinitionLabelFactory">
         </service>
      </serviceFactory>
   </extension>

</plugin>
