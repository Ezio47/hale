<?xml version="1.0" encoding="UTF-8"?>
<!-- TUD mdv 2009: version in accordance with OMWG D7.2: Ontology Mapping Language RDF/XML Syntax
DERI OMWG Working Draft last update 21 January 2007
Changes:
- now 2 namespaces: omwg: and align:
- added the align: namespaces to the element names
- the omwg (= default) namespace does not have a forward slash at the end anymore
- added rdf:about attribute to Alignment element
- added rdf:datatype="xsd:float" to 'measure'
-->
<align:Alignment rdf:about="" xmlns="http://www.omwg.org/TR/d7/ontology/alignment"
    xmlns:omwg="http://www.omwg.org/TR/d7/ontology/alignment"
    xmlns:align="http://knowledgeweb.semanticweb.org/heterogeneity/alignment"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://knowledgeweb.semanticweb.org/heterogeneity/alignment align_d7.2.xsd">
    <align:level>2OMWG</align:level>
    <align:onto1>
        <align:Ontology rdf:about="vector25_vogis_atkis">
            <align:location>inputdatasets.xsd</align:location>
            <align:formalism>
                <align:Formalism>
                    <align:uri/>
                    <align:name>XSD</align:name>
                </align:Formalism>
            </align:formalism>
        </align:Ontology>
    </align:onto1>
    <align:onto2>
        <align:Ontology rdf:about="euroroads">
            <align:location>euroroads.xsd</align:location>
            <align:formalism>
                <align:Formalism>
                    <align:uri/>
                    <align:name>XSD</align:name>
                </align:Formalism>
            </align:formalism>
        </align:Ontology>
    </align:onto2>
    <align:map>
        <align:Cell rdf:about="1_renaming_ex1">
            <entity1>
                <Class rdf:about="vector25:Strasse"/>
            </entity1>
            <entity2>
                <Class rdf:about="euroroads:RoadLink"/>
            </entity2>
            <align:measure rdf:datatype="xsd:float">1</align:measure>
            <align:relation>Equivalence</align:relation>
        </align:Cell>
    </align:map>
    <align:map>
        <align:Cell rdf:about="1_renaming_ex2">
            <entity1>
                <Property rdf:about="vector25:OBJECTID">
                    <domainRestriction>
                        <Class rdf:about="vector25:Strasse"/>
                    </domainRestriction>
                </Property>
            </entity1>
            <entity2>
                <Property rdf:about="er:id/er:permanentId">
                    <domainRestriction>
                        <Class rdf:about="er:RoadLink"/>
                    </domainRestriction>
                </Property>
            </entity2>
            <align:measure rdf:datatype="xsd:float">1</align:measure>
            <align:relation>Equivalence</align:relation>
        </align:Cell>
    </align:map>
    <align:map>
        <align:Cell rdf:about="2_renaming_ex3">
            <entity1>
                <Class rdf:about="vogis:Streets"/>
            </entity1>
            <entity2>
                <Class rdf:about="er:RoadLink"/>
            </entity2>
            <align:measure rdf:datatype="xsd:float">1</align:measure>
            <align:relation>Equivalence</align:relation>
        </align:Cell>
    </align:map>
    <align:map>
        <align:Cell rdf:about="2_simple_reclassification_ex1_1">
            <entity1>
                <Property rdf:about="vogis:STR_TYP">
                    <domainRestriction>
                        <Class rdf:about="vogis:Streets"/>
                    </domainRestriction>
                    <valueCondition>
                        <Restriction>
                            <comparator>equal</comparator>
                            <value><literal>E</literal></value>
                        </Restriction>
                    </valueCondition>
                </Property>
            </entity1>
            <entity2>
                <Property rdf:about="er:formOfWay">
                    <domainRestriction>
                        <Class rdf:about="er:RoadLink"/>
                    </domainRestriction>
                    <valueCondition>
                        <Restriction>
                            <comparator>equal</comparator>
                            <value><literal>SingleCarriageway</literal></value>
                        </Restriction>
                    </valueCondition>
                </Property>
            </entity2>
            <align:measure rdf:datatype="xsd:float">1</align:measure>
            <align:relation>Equivalence</align:relation>
        </align:Cell>
    </align:map>
    <!-- alternative: 1. use Class and not Property; 2. give 'map' the id + have more then 1 Cell per 'map' -->
    <align:map rdf:about="2_simple_reclassification_ex1_alternative">
        <align:Cell>
            <entity1>
                <Class rdf:about="vogis:Streets">
                    <attributeValueCondition>
                        <Restriction>
                            <onAttribute>
                                <Property rdf:about="vogis:STR_TYP"/>
                            </onAttribute>
                            <comparator>equal</comparator>
                            <value><literal>E</literal></value>
                        </Restriction>
                    </attributeValueCondition>
                </Class>
            </entity1>
            <entity2>
                <Class rdf:about="er:RoadLink">
                    <attributeValueCondition>
                        <Restriction>
                            <onAttribute>
                                <Property rdf:about="er:formOfWay"/>
                            </onAttribute>
                            <comparator>equal</comparator>
                            <value><literal>SingleCarriageway</literal></value>
                        </Restriction>
                    </attributeValueCondition>
                </Class>
            </entity2>
            <align:measure rdf:datatype="xsd:float">1</align:measure>
            <align:relation>Equivalence</align:relation>
        </align:Cell>
        <align:Cell>
            <entity1>
                <Class rdf:about="vogis:Streets">
                    <attributeValueCondition>
                        <Restriction>
                            <onAttribute>
                                <Property rdf:about="vogis:STR_TYP"/>
                            </onAttribute>
                            <comparator>oneOf</comparator>
                            <!-- mdv 2008 added oneOf  -->
                            <!--value><literal>"G","A","F"</literal></value-->
                            <value><literal>G</literal></value>
                            <value><literal>A</literal></value>
                            <value><literal>F</literal></value>
                        </Restriction>
                    </attributeValueCondition>
                </Class>
            </entity1>
            <entity2>
                <Class rdf:about="er:RoadLink">
                    <attributeValueCondition>
                        <Restriction>
                            <onAttribute>
                                <Property rdf:about="er:formOfWay"/>
                            </onAttribute>
                            <comparator>equal</comparator>
                            <value><literal>DualCarriageway</literal></value>
                        </Restriction>
                    </attributeValueCondition>
                </Class>
            </entity2>
            <align:measure rdf:datatype="xsd:float">0.33</align:measure>
            <align:relation>SubsumedBy</align:relation>
            <!-- mdv 2008: assumption is that the relation is read like this: Entity1 SubsumedBy Entity2 -->
            <!-- reverse transformation is not possible here, unless a default target value is set for reverse transformation -->
            <!-- TODO: extension/change needed ?-->
        </align:Cell>
    </align:map>
    <align:map>
        <align:Cell rdf:about="3_simple_type_change_ex1">
            <entity1>
                <Property rdf:about="atkis:BRV">
                    <domainRestriction>
                        <Class rdf:about="atkis:StrasseTeil"/>
                    </domainRestriction>
                    <typeCondition>xs:string</typeCondition>
                    <transf rdf:resource="transf:toNumber"/>
                </Property>
            </entity1>
            <entity2>
                <Property rdf:about="er:roadWidth">
                    <label>uom=meter</label>
                    <domainRestriction>
                        <Class rdf:about="er:RoadLink"/>
                    </domainRestriction>
                    <typeCondition>xs:number</typeCondition>
                    <!--transf rdf:resource="transf:toString"/-->
                    <!-- TODO: specify reverse transf here ? -->
                </Property>
            </entity2>
            <align:measure rdf:datatype="xsd:float">1</align:measure>
            <align:relation>Equivalence</align:relation>
        </align:Cell>
    </align:map>
    <align:map rdf:about="4_complex_reclassification_ex1">
        <align:Cell>
            <entity1>
                <Class rdf:about="vector25:Strasse">
                    <attributeValueCondition>
                        <Restriction>
                            <onAttribute>
                                <Property rdf:about="vector25:OBJECTVAL"/>
                            </onAttribute>
                            <comparator>equal</comparator>
                            <value><literal>Autobahn</literal></value>
                        </Restriction>
                    </attributeValueCondition>
                </Class>
            </entity1>
            <entity2>
                <Class rdf:about="er:RoadLink">
                    <attributeValueCondition>
                        <Restriction>
                            <onAttribute>
                                <Property rdf:about="er:formOfWay"/>
                            </onAttribute>
                            <comparator>equal</comparator>
                            <value><literal>Motorway</literal></value>
                        </Restriction>
                    </attributeValueCondition>
                    <attributeValueCondition>
                        <Restriction>
                            <onAttribute>
                                <Property rdf:about="er:functionalRoadClass"/>
                            </onAttribute>
                            <comparator>equal</comparator>
                            <value><literal>MainRoad</literal></value>
                        </Restriction>
                    </attributeValueCondition>
                </Class>
            </entity2>
            <align:measure rdf:datatype="xsd:float">1</align:measure>
            <align:relation>Equivalence</align:relation>
        </align:Cell>
        <align:Cell>
            <entity1>
                <Class rdf:about="vector25:Strasse">
                    <attributeValueCondition>
                        <Restriction>
                            <onAttribute>
                                <Property rdf:about="vector25:OBJECTVAL"/>
                            </onAttribute>
                            <comparator>equal</comparator>
                            <value><literal>Autostr</literal></value>
                        </Restriction>
                    </attributeValueCondition>
                </Class>
            </entity1>
            <entity2>
                <Class rdf:about="er:RoadLink">
                    <attributeValueCondition>
                        <Restriction>
                            <onAttribute>
                                <Property rdf:about="er:formOfWay"/>
                            </onAttribute>
                            <comparator>equal</comparator>
                            <value><literal>Freeway</literal></value>
                        </Restriction>
                    </attributeValueCondition>
                    <attributeValueCondition>
                        <Restriction>
                            <onAttribute>
                                <Property rdf:about="er:functionalRoadClass"/>
                            </onAttribute>
                            <comparator>equal</comparator>
                            <value><literal>MainRoad</literal></value>
                        </Restriction>
                    </attributeValueCondition>
                </Class>
            </entity2>
            <align:measure rdf:datatype="xsd:float">1</align:measure>
            <align:relation>Equivalence</align:relation>
        </align:Cell>
        <align:Cell>
            <entity1>
                <Class rdf:about="vector25:Strasse">
                    <attributeValueCondition>
                        <Restriction>
                            <onAttribute>
                                <Property rdf:about="vector25:OBJECTVAL"/>
                            </onAttribute>
                            <comparator>equal</comparator>
                            <value><literal>Nebenst</literal></value>
                        </Restriction>
                    </attributeValueCondition>
                </Class>
            </entity1>
            <entity2>
                <Class rdf:about="er:RoadLink">
                    <attributeValueCondition>
                        <Restriction>
                            <onAttribute>
                                <Property rdf:about="er:formOfWay"/>
                            </onAttribute>
                            <comparator>equal</comparator>
                            <value><literal>SingleCarriageway</literal></value>
                        </Restriction>
                    </attributeValueCondition>
                    <attributeValueCondition>
                        <Restriction>
                            <onAttribute>
                                <Property rdf:about="er:functionalRoadClass"/>
                            </onAttribute>
                            <comparator>equal</comparator>
                            <value><literal>ThirdClass</literal></value>
                        </Restriction>
                    </attributeValueCondition>
                </Class>
            </entity2>
            <align:measure rdf:datatype="xsd:float">1</align:measure>
            <align:relation>Equivalence</align:relation>
        </align:Cell>
    </align:map>
    <align:map rdf:about="5_multi-attribute_conditions_ex1">
        <align:Cell>
            <entity1>
                <Property rdf:about="atkis:BRF">
                    <domainRestriction>
                        <Class rdf:about="atkis:StrasseTeil"/>
                    </domainRestriction>
                    <valueCondition>
                        <Restriction>
                            <!-- mdv 2008 added 'between' -->
                            <comparator>between</comparator>
                            <value><min>0</min></value>
                            <value><max>9998</max></value>
                        </Restriction>
                    </valueCondition>
                    <transf rdf:resource="transf:multiplyBy">
                        <params>0.1</params>
                    </transf>
                </Property>
            </entity1>
            <entity2>
                <Property rdf:about="er:roadWidth">
                    <label>uom=meter</label>
                    <domainRestriction>
                        <Class rdf:about="er:RoadLink"/>
                    </domainRestriction>
                    <!--valueCondition>
                        <Restriction>
                            <comparator>not-in</comparator>
                            <value><literal>6</literal></value>
                            <value><literal>9</literal></value>
                            <value><literal>12</literal></value>
                        </Restriction>
                    </valueCondition-->
                    <typeCondition>xs:xsd:float</typeCondition>
                    <!-- TODO: hint that reverse only possible if atkis:BRV is also given a value  -->
                </Property>
            </entity2>
            <align:measure rdf:datatype="xsd:float">0.5</align:measure>
            <align:relation>SubsumedBy</align:relation>
        </align:Cell>
        <align:Cell>
            <entity1>
                <Property rdf:about="atkis:BRV">
                    <!-- disjoint with previous condition -->
                    <not>
                        <Property rdf:about="atkis:BRF">
                            <valueCondition>
                                <Restriction>
                                    <!-- mdv 2008 added 'between' -->
                                    <comparator>between</comparator>
                                    <value><min>0</min></value>
                                    <value><max>9998</max></value>
                                </Restriction>
                            </valueCondition>
                        </Property>
                    </not>
                    <domainRestriction>
                        <Class rdf:about="atkis:StrasseTeil"/>
                    </domainRestriction>
                    <valueCondition>
                        <Restriction>
                            <!-- mdv 2008 added 'oneOf' -->
                            <comparator>oneOf</comparator>
                            <value><literal>6</literal></value>
                            <value><literal>9</literal></value>
                            <value><literal>12</literal></value>
                        </Restriction>
                    </valueCondition>
                    <typeCondition>xs:string</typeCondition>
                    <transf rdf:resource="transf:toNumber"/>
                </Property>
            </entity1>
            <entity2>
                <Property rdf:about="er:roadWidth">
                    <label>uom=meter</label>
                    <domainRestriction>
                        <Class rdf:about="er:RoadLink"/>
                    </domainRestriction>
                    <valueCondition>
                        <Restriction>
                            <!-- mdv 2008 added 'oneOf' -->
                            <comparator>oneOf</comparator>
                            <value><literal>6</literal></value>
                            <value><literal>9</literal></value>
                            <value><literal>12</literal></value>
                        </Restriction>
                    </valueCondition>
                    <typeCondition>xs:xsd:float</typeCondition>
                    <!-- TODO: reverse transf is possible in this case -->
                </Property>
            </entity2>
            <align:measure rdf:datatype="xsd:float">1</align:measure>
            <align:relation>Equivalence</align:relation>
        </align:Cell>
    </align:map>
    <align:map>
        <align:Cell rdf:about="6_merge_features_ex1">
            <entity1>
                <Property rdf:about="atkis:the_geom">
                    <domainRestriction>
                        <Class rdf:about="atkis:StrasseTeil"/>
                    </domainRestriction>
                    <typeCondition>gml:MultiLineString</typeCondition>
                    <!-- TODO mdv change schema, and think about how to specify complete set of steps 
                        (also handling of thematic attributes after group-by) -->
                    <transfPipe>
                        <transf rdf:resource="transf:group-by">
                            <params>atkis:OBJNR</params>
                        </transf>
                        <transf rdf:resource="transf:concatLineGeom"/>
                    </transfPipe>
                </Property>
            </entity1>
            <entity2>
                <Property rdf:about="er:curve">
                    <domainRestriction>
                        <Class rdf:about="er:RoadLink"/>
                    </domainRestriction>
                    <typeCondition>gml:LineString</typeCondition>
                    <!-- TODO: hint no reverse transf here (split), apart from simple change of type to multi -->
                </Property>
            </entity2>
            <align:measure rdf:datatype="xsd:float">1</align:measure>
            <!-- TODO mdv added -->
            <align:relation>PartOf</align:relation>
        </align:Cell>
    </align:map>
    <!--align:map>
        <align:Cell rdf:about="7_split_features_ex1" />
    </align:map>
    <align:map>
        <align:Cell rdf:about="8_geometric_operations_ex1" />
    </align:map-->

</align:Alignment>
