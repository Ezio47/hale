<?xml version="1.0" encoding="UTF-8"?>
<!-- TUD mdv 2009: version in accordance with OMWG D7.2: Ontology Mapping Language RDF/XML Syntax
DERI OMWG Working Draft last update 21 January 2007
Changes:
- now 2 namespaces: omwg: and align:
- added the align: namespaces to the element names
- the omwg (= default) namespace does not have a forward slash at the end anymore
- added rdf:about attribute to Alignment element
- added rdf:datatype="xsd:float" to 'measure'
-->
<align:Alignment rdf:about="" xmlns="http://www.omwg.org/TR/d7/ontology/alignment"
    xmlns:omwg="http://www.omwg.org/TR/d7/ontology/alignment"
    xmlns:align="http://knowledgeweb.semanticweb.org/heterogeneity/alignment"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://knowledgeweb.semanticweb.org/heterogeneity/alignment align_d7.2.xsd">
    <align:level>2OMWG</align:level>
    <align:onto1>
        <align:Ontology rdf:about="waterBW">
            <align:location>inputdatasets.xsd</align:location>
            <align:formalism>
                <align:Formalism>
                    <align:uri/>
                    <align:name>XSD</align:name>
                </align:Formalism>
            </align:formalism>
        </align:Ontology>
    </align:onto1>
    <align:onto2>
        <align:Ontology rdf:about="inspire">
            <align:location>hydrography.xsd</align:location>
            <align:formalism>
                <align:Formalism>
                    <align:uri/>
                    <align:name>XSD</align:name>
                </align:Formalism>
            </align:formalism>
        </align:Ontology>
    </align:onto2>

    <align:map>
        <align:Cell rdf:about="" operation="featureRenameWithFilter">
            <entity1>
                <FeatureClass rdf:about="waterBW:lakesBW">
                    <attributeValueCondition>
                        <Restriction>
                            <!--onAttribute>
                                <Property rdf:about="waterBW:OBJART"/>
                            </onAttribute>
                            <comparator>equal</comparator>
                            <value><literal>5112</literal></value-->

                            <cqlStr>waterBW:OBJART=5112</cqlStr>

                        </Restriction>
                    </attributeValueCondition>
                </FeatureClass>
            </entity1>
            <entity2>
                <FeatureClass rdf:about="inspire:StandingWater">
                </FeatureClass>
            </entity2>
            <align:measure rdf:datatype="xsd:float">1</align:measure>
            <align:relation>Equivalence</align:relation>
        </align:Cell>
    </align:map>

    <align:map>
        <align:Cell rdf:about="" operation="attributeRename">
            <entity1>
                <Property rdf:about="waterBW:GN">
                    <domainRestriction>
                        <FeatureClass rdf:about="waterBW:lakesBW"/>
                    </domainRestriction>
                </Property>
            </entity1>
            <entity2>
                <Property rdf:about="inspire:geographicalName/inspire:spelling/inspire:text">
                    <domainRestriction>
                        <FeatureClass rdf:about="inspire:StandingWater"/>
                    </domainRestriction>
                </Property>
            </entity2>
            <align:measure rdf:datatype="xsd:float">1</align:measure>
            <align:relation>Equivalence</align:relation>
        </align:Cell>
    </align:map>

    <align:map>
        <align:Cell rdf:about="" operation="setDefaultValue">
            <entity1>
                <Property rdf:about="">
                </Property>
            </entity1>
            <entity2>
                <Property rdf:about="inspire:geographicalName/inspire:spelling/inspire:script">
                    <domainRestriction>
                        <FeatureClass rdf:about="inspire:StandingWater"/>
                    </domainRestriction>
                    <valueCondition>
                        <Restriction>
                            <comparator>equal</comparator>
                            <value><literal>Latin</literal></value>
                        </Restriction>
                    </valueCondition>
                </Property>
            </entity2>
            <align:measure rdf:datatype="xsd:float">1</align:measure>
            <align:relation>Difference</align:relation>
        </align:Cell>
    </align:map>

    <align:map>
        <align:Cell rdf:about="" operation="setDefaultValue">
            <entity1>
                <Property rdf:about="">
                </Property>
            </entity1>
            <entity2>
                <Property rdf:about="inspire:geographicalName/inspire:language">
                    <domainRestriction>
                        <FeatureClass rdf:about="inspire:StandingWater"/>
                    </domainRestriction>
                    <valueCondition>
                        <Restriction>
                            <comparator>equal</comparator>
                            <value><literal>ger</literal></value>
                        </Restriction>
                    </valueCondition>
                </Property>
            </entity2>
            <align:measure rdf:datatype="xsd:float">1</align:measure>
            <align:relation>Difference</align:relation>
        </align:Cell>
    </align:map>


    <align:map>
        <align:Cell rdf:about="" operation="setDefaultValue">
            <entity1>
                <Property rdf:about="">
                </Property>
            </entity1>
            <entity2>
                <Property rdf:about="inspire:geographicalName/inspire:status">
                    <domainRestriction>
                        <FeatureClass rdf:about="inspire:StandingWater"/>
                    </domainRestriction>
                    <valueCondition>
                        <Restriction>
                            <comparator>equal</comparator>
                            <value><literal>Official</literal></value>
                        </Restriction>
                    </valueCondition>
                </Property>
            </entity2>
            <align:measure rdf:datatype="xsd:float">1</align:measure>
            <align:relation>Difference</align:relation>
        </align:Cell>
    </align:map>

    <align:map>
        <align:Cell rdf:about="" operation="MultiPolygon2Polygon_splitFeature" >
            <entity1>
                <Property rdf:about="waterBW:the_geom">
                    <domainRestriction>
                        <FeatureClass rdf:about="waterBW:lakesBW"/>
                    </domainRestriction>
                    <typeCondition>gml:MultiPolygonPropertyType</typeCondition>
                    <transf rdf:resource="humboldt:MultiPolygon2Polygon"/>
                </Property>
            </entity1>
            <entity2>
                <Property rdf:about="inspire:geometry">
                    <domainRestriction>
                        <FeatureClass rdf:about="inspire:StandingWater"/>
                    </domainRestriction>
                    <typeCondition>gml:SurfacePropertyType</typeCondition>
                </Property>
            </entity2>
            <align:measure rdf:datatype="xsd:float">1</align:measure>
            <!-- TODO mdv added -->
            <align:relation>PartOf</align:relation>
        </align:Cell>
    </align:map>

    <align:map>
        <align:Cell rdf:about="" operation="setDefaultValue">
            <entity1>
                <Property rdf:about="">
                </Property>            
            </entity1>
            <entity2>
                <Property rdf:about="inspire:hydroId/inspire:countryCode">
                    <domainRestriction>
                        <FeatureClass rdf:about="inspire:StandingWater"/>
                    </domainRestriction>
                    <valueCondition>
                        <Restriction>
                            <comparator>equal</comparator>
                            <value><literal>DE</literal></value>
                        </Restriction>
                    </valueCondition>
                </Property>
            </entity2>
            <align:measure rdf:datatype="xsd:float">1</align:measure>
            <align:relation>Difference</align:relation>
        </align:Cell>
    </align:map>

    <align:map>
        <align:Cell rdf:about="" operation="renameAttribute">
            <entity1>
                <Property rdf:about="waterBW:KN">
                    <domainRestriction>
                        <FeatureClass rdf:about="waterBW:lakesBW"/>
                    </domainRestriction>
                </Property>
            </entity1>
            <entity2>
                <Property rdf:about="inspire:hydroId/inspire:id">
                    <domainRestriction>
                        <FeatureClass rdf:about="inspire:StandingWater"/>
                    </domainRestriction>
                </Property>
            </entity2>
            <align:measure rdf:datatype="xsd:float">1</align:measure>
            <align:relation>Equivalence</align:relation>
        </align:Cell>
    </align:map>


    <align:map>
        <align:Cell rdf:about="" operation="setDefaultValue">
            <entity1>               
                <Property rdf:about="">
                </Property>           
            </entity1>
            <entity2>
                <Property rdf:about="inspire:levelOfDetail">
                    <domainRestriction>
                        <FeatureClass rdf:about="inspire:StandingWater"/>
                    </domainRestriction>
                    <valueCondition>
                        <Restriction>
                            <comparator>equal</comparator>
                            <value><literal>Regional</literal></value>
                        </Restriction>
                    </valueCondition>
                </Property>
            </entity2>
            <align:measure rdf:datatype="xsd:float">1</align:measure>
            <align:relation>Difference</align:relation>
        </align:Cell>
    </align:map>

    <align:map>
        <align:Cell rdf:about="" operation="setDefaultValue">
            <entity1>
                <Property rdf:about="">
                </Property>                       
            </entity1>
            <entity2>
                <Property rdf:about="inspire:localType">
                    <domainRestriction>
                        <FeatureClass rdf:about="inspire:StandingWater"/>
                    </domainRestriction>
                    <valueCondition>
                        <Restriction>
                            <comparator>equal</comparator>
                            <value><literal>Binnensee, Stausee, Teich</literal></value>
                        </Restriction>
                    </valueCondition>
                </Property>
            </entity2>
            <align:measure rdf:datatype="xsd:float">1</align:measure>
            <align:relation>Difference</align:relation>
        </align:Cell>
    </align:map>



</align:Alignment>
